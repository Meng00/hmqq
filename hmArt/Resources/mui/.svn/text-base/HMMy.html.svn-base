<script>
	
	if(mui.os.ios){
   		mui('.mui-content')[0].className = mui('.mui-content')[0].className + ' paddingt-138';
   	}
	var currsign = "";
	var winLoad = {
		init :function(){
			winLoad.checkLogin();
			HMARTJsObj.getAppInfo('winLoad.getAppInfo');
			app.getHardwareInfo('winLoad.getHardwareInfo');
			mui('ul').off('tap');
			mui('ul').on('tap','li',function(){
				var href = this.getAttribute('href');
				if(href == 'tel'){
					app.phoneCallByApp();
				}else{
					if(mobile){
						app.pushView(href);
					}else{
						winLoad.checkLogin();
					}
				}
			});
			mui('.mui-pull-left')[0].addEventListener('tap',function(){
				if(mobile){
					var url = 'hmqq-jsweb://localhost/mui/member/HMSetting.html';
					app.pushView(url);
				}else{
					winLoad.checkLogin();
				}
			});
			mui('.name')[0].addEventListener('tap',function(){
				if(mobile){
					
				}else{
					winLoad.checkLogin();
				}
			});
//			mui('.vip')[0].addEventListener('tap',function(){
//				if(mobile){
//					var url = 'hmqq-jsweb://localhost/mui/member/HMDeposit.html';
//					app.pushView(url);
//				}else{
//					winLoad.checkLogin();
//				}
//			});
			api.other.sysVersion.DATA.device =  hmqq_device;
			app.HMARTJsObj(api.other.sysVersion.DATA,'winLoad.getVersion');

		}
		,checkLogin :function(){
			app.checkLogin(1,'winLoad.checkLoginCallback');
		},
		getLoginMobile : function(data){ 
			api.my.getLoginMobile.param = eval('(' + data +')');
			app.ajax(api.my.getLoginMobile,function(data){
				if(data.result == '1'){
					if(data.seller){
						mui('.seller')[0].setAttribute('href','hmqq-remote://mui/seller/HMHome.html');
					}
//					if(data.photo){
//						mui('#photo')[0].setAttribute('src',config.HOST_IMG+data.photo);
//					}
					if(data.distributor){
						mui('.distr')[0].setAttribute('href','hmqq-mui://app.4008988518.com/art-interface/mui/distr/HMDistrEntrance.html');
					}else{
						mui('.distr')[0].setAttribute('href','hmqq-mui://app.4008988518.com/art-interface/mui/distr/HMDistrAdd.html');
					}
				}
			});
		},
		checkLoginCallback : function (data){
	   		var json = eval('(' + data +')');
	   		if(json.loginStatus == '1'){
				mobile = json.mobile;
				vip = 0;
				mui('.name')[0].innerHTML = json.mobile;
				if(json.vip == 1){
					vip = 1;
					mui('.vip')[0].innerHTML = 'VIP';
				}
				app.HMARTJsObj(api.my.getCount.DATA,'winLoad.getCount');
				app.HMARTJsObj(api.my.getLoginMobile.DATA,'winLoad.getLoginMobile');
			}	 
		},
		getAppInfo: function(data){
			var json = eval('(' + data +')');
	   		api.other.appVersion.cuurVer = json.version;
	   		mui('.version')[0].innerHTML = 'V'+json.version;
	   		if(mui.os.ios){
	   			api.other.appVersion.DATA.type = 0;
	   		}else api.other.appVersion.DATA.type = 1;
	   		app.HMARTJsObj(api.other.appVersion.DATA,'winLoad.appVersion'); 
		},
		getCount : function(data){
			api.my.getCount.param = eval('(' + data +')');
			app.ajax(api.my.getCount,function(data){
				if(data.result == '1'){
					mui('.em1')[0].style.display = 'none';
					mui('.em2')[0].style.display = 'none';
					mui('.em3')[0].style.display = 'none';
					mui('.em4')[0].style.display = 'none';
					
					 if(data.obj.count1>0){
					 	mui('.em1')[0].style.display = '';
					 }if(data.obj.count2>0){
					 	mui('.em2')[0].style.display = '';
					 }if(data.obj.count3>0){
					 	mui('.em3')[0].style.display = '';
					 }if(data.obj.count4>0){
					 	mui('.em4')[0].style.display = '';
					 }
				}
			});
		},
		getHardwareInfo :function(data){
			var json = eval('(' + data +')');
			api.push.getCount.DATA.deviceId = json.deviceId;
			app.HMARTJsObj(api.push.getCount.DATA, 'winLoad.pushCount');
		},
		pushCount : function(data){
			api.push.getCount.param = eval('(' + data +')');
			app.ajax(api.push.getCount,function(data){
				if(data.obj > 0){
					mui('.mui-badge')[0].innerHTML = '';
					mui('.mui-badge')[0].style.display = '';
				}
			});
		},
		appVersion : function(data){
			api.other.appVersion.param = eval('(' + data +')');
			app.ajax(api.other.appVersion,function(data){
				if(data.result == '1'){
					if(api.other.appVersion.cuurVer != data.obj.version){
						mui('.version_li')[0].setAttribute('href','hmqq-http://active.clewm.net/ASNpFv?qrurl=c3.cli.im%2FASNpFv&gtype=1&scant=1&uname=30%2A%2A%2A%2A%40qq.com&uuid=57872d7b69e1570c574c230c&token=243395d047eeabb7851488a810a95d46');
					}
				}
			});
		},
		getVersion: function(data){
			api.other.sysVersion.param = eval('(' + data +')');
			app.ajax(api.other.sysVersion,function(data){
				currsign = data.obj.device+'_'+data.obj.version;
				loadJs(config.HOST+'mui/apppage/my_'+currsign+'.js')
			})
		}
	}
    //app通知
	function onARTJsObjReady(){
		mui('.mui-scroll-wrapper').scroll({
			 deceleration:deceleration, //阻尼系数,系数越小滑动越灵敏
			 indicators: true //是否显示滚动条
		});
		mobile = null;
		mui('.name')[0].innerHTML = '登录/注册';
//		mui('.vip')[0].innerHTML = '升级VIP';
		winLoad.init();
		loadding = false;
	}
   	
   	function pageRefresh(){
		mobile = null;
		mui('.name')[0].innerHTML = '登录/注册';
//		mui('.vip')[0].innerHTML = '升级VIP';
		HMARTJsObj.checkLogin(0,'winLoad.checkLoginCallback');
	}
   	
   	Mock.mock(config.HOST+config.MOCK_KEY+api.my.getCount.URL,{"result":"1","message":"加载成功","obj":{}});
	Mock.mock(config.HOST+config.MOCK_KEY+api.push.getCount.URL,{"result":"1","message":"加载成功","obj":9});
	app.load.mock(function(){});
</script>


