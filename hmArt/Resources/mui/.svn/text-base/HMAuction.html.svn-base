		function pageRefresh(){
			if(typeof(pageIndex) == "undefined"){
				loadding = false;
				api.store.search.DATA.pageIndex = 1;
				api.store.search.DATA.other.type = 1;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction1');
				api.store.search.DATA.other.type = 2;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction2');
				api.store.search.DATA.other.type = 3;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction3');
				loadding = true;
			}else{
				delete pageIndex;
			}
		}
