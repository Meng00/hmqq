	<script src="js/mui.pullToRefresh.js"></script>
	<script src="js/mui.pullToRefresh.material.js"></script>
	<script>
		if(mui.os.ios){
			mui('.mui-content')[0].classList.add('paddingt_146');
		}else{
			mui('.mui-content')[0].classList.add('paddingt_126');
		}
		winBind = {
			init : function(){
				winBind.onMobile();
				mui('header').on('tap','.mui-icon-clear',function(){
					api.store.search.DATA.other.keywork = mui('#search1')[0].value;
					winLoad.initData();
				});
				var scroll = mui('#wrapper1').scroll();
				document.querySelector('#wrapper1').addEventListener('scroll', function (e) { 
					scroll_y = -scroll.y;	
					if(scroll_y < 50){
						mui('.scroll_init')[0].classList.add('hidden');
					}else{
						mui('.scroll_init')[0].classList.remove('hidden');
					}
				})
				var scroll2 = mui('#wrapper2').scroll();
				document.querySelector('#wrapper2').addEventListener('scroll', function (e) { 
					scroll_y = -scroll2.y;	
					if(scroll_y < 50){
						mui('.scroll_init')[0].classList.add('hidden');
					}else{
						mui('.scroll_init')[0].classList.remove('hidden');
					}
				})
				mui('.scroll_init')[0].setAttribute('style', 'top:'+(document.body.scrollHeight-80)+'px');
			},
			onMobile : function(){
				mui('.tab_sec').on('tap','li',function(){
					api.store.search.DATA.other.keywork = mui('#search1')[0].value;
					var index = this.getAttribute('index');
					if(index == 1 || index == 4 ){

						mui('.tab_sec')[0].querySelector('.active').classList.remove('active');
						this.firstChild.classList.add('active');
						api.store.search.DATA.other.sort = index-1;
						api.store.search.DATA.other.order = '';
						winLoad.initData();
						mui('.order')[0].className = 'order';
						mui('.order')[1].className = 'order';
					}else if(index == 5){
						if(this.querySelector('.mui-slider').style.display == 'none'){
							this.classList.add('bg_f4');
							this.querySelector('.mui-slider').style.display = '';
						}else{
							this.classList.remove('bg_f4');
							this.querySelector('.mui-slider').style.display = 'none';
						}
					}
				});
				mui('#topPopover').on('tap','li',function(){
					mui('#topPopover')[0].classList.remove('mui-active');
					mui('#topPopover')[0].style.display = 'none';
					mui('.mui-backdrop')[0].remove();
					mui('.tab_sec')[0].querySelector('.active').classList.remove('active');
					mui('.order')[0].parentNode.classList.add('active');
					var orderBy = this.getAttribute('orderBy');
					if(orderBy == 'desc'){
						mui('.order')[0].className = 'order dot_bottom';
					}else {
						mui('.order')[0].className = 'order dot_top';
					}
					mui('.order')[1].className = 'order';
					api.store.search.DATA.other.sort = 1;
					api.store.search.DATA.other.order = orderBy;
					winLoad.initData();
				});
				mui('#topPopover2').on('tap','li',function(){
					mui('#topPopover2')[0].classList.remove('mui-active');
					mui('#topPopover2')[0].style.display = 'none';
					mui('.mui-backdrop')[0].remove();
					mui('.tab_sec')[0].querySelector('.active').classList.remove('active');
					mui('.order')[1].parentNode.classList.add('active');
					var orderBy = this.getAttribute('orderBy');
					if(orderBy == 'desc'){
						mui('.order')[1].className = 'order dot_bottom';
					}else {
						mui('.order')[1].className = 'order dot_top';
					}
					mui('.order')[0].className = 'order';
					api.store.search.DATA.other.sort = 2;
					api.store.search.DATA.other.order = orderBy;
					winLoad.initData();
				});
//				mui('#search')[0].addEventListener('tap',function(){
//					api.store.search.DATA.other.keywork = mui('#search1')[0].value;
//					if(mui('.limit_top')[0].querySelector('.active')){
//						var code = mui('.limit_top')[0].querySelector('.active').getAttribute('code');
//						api.store.search.DATA.other.classifyCode = (code==null?'':code);
//					} 
//					winLoad.initData();
//				});
			},
			toArtInfo: function(){
				mui('.pull ul').off('tap');
				mui('.pull ul').on('tap','li',function(){
				 	var ownerId = this.getAttribute('ownerId');
					var url = 'hmqq-jsweb://localhost/mui/HMArtInfo.html?ownerId='+ownerId;
					app.pushView(url);
				});
				app.DateUtil.initCountdown();
			}
		}
		
		winLoad = {
			init : function(){
				/* 容器最小高度 */
				mui('.mui-control-content').each(function(){
					this.style.minHeight = window.innerHeight - 126+"px";// - 50 
				});
				//循环初始化所有下拉刷新，上拉加载。
				mui.each(document.querySelectorAll('.pull .mui-scroll'), function(index, pullRefreshEl) {
					mui(pullRefreshEl).pullToRefresh({
						down: {
							callback: false
						},
						up: {
							contentinit: '',
							contentdown: '',
							callback: function() {
								var self = this;
								var ul = self.element.querySelector('.pull-scroll');
								var pageIndex = ul.getAttribute('pageIndex');
								var type = ul.getAttribute('data-type');
//								alert(type+" "+pageIndex);
								api.store.search.DATA.pageIndex = (Number(pageIndex)|| 1) +1; //下一页
								api.store.search.DATA.other.type = type;
								pageIndex = api.store.search.DATA.pageIndex;
//								alert(JSON.stringify(api.store.search.DATA));
								winLoad.pullScroll = function(data){
									api.store.search.param = eval('(' + data +')');
									app.ajax(api.store.search,function(data){
										var isEnd = false;
										if(data.data.length > 0){
											//alert(type);
											if(pageIndex == 1){
												ul.innerHTML = '';
											}
											var div = document.createElement("div");
										　　if(type == 3 || type == 2){
												div.innerHTML = template('temp-auction-03', data);
											}else{
												div.innerHTML = template('temp-auction-01', data);
											}
											ul.appendChild(div);
											ul.setAttribute('pageIndex', pageIndex);
											//mui('#search')[0].innerHTML = +type+":"+ul.id+":"+pageIndex;
											winBind.toArtInfo();
										}else isEnd = true;
										self.endPullUpToRefresh();
									});
								}
								app.HMARTJsObj(api.store.search.DATA,'winLoad.pullScroll');
								setTimeout(function() {
									self.endPullUpToRefresh();
								}, 30000);
							}
						}
					});
				});
				/* 加载模块 */
				api.store.search.DATA.other.type = 1;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction1');
				api.store.search.DATA.other.type = 2;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction2');
				api.store.search.DATA.other.type = 3;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction3');
				app.HMARTJsObj(api.store.classfiy.DATA,'winLoad.classfiy');
			},
			initData : function(){
				mui("#scroll1")[0].setAttribute('pageIndex', 0);
				mui("#scroll2")[0].setAttribute('pageIndex', 0);
				mui("#scroll3")[0].setAttribute('pageIndex', 0);
//				mui('#wrapper1').pullRefresh().refresh(true);
//				mui('#wrapper2').pullRefresh().refresh(true);
//				mui('#wrapper3').pullRefresh().refresh(true);
				mui('#wrapper1').scroll().scrollTo(0,0,100);//
				mui('#wrapper2').scroll().scrollTo(0,0,100);//
				mui('#wrapper3').scroll().scrollTo(0,0,100);//
				
				api.store.search.DATA.pageIndex = 1;
				api.store.search.DATA.other.type = 1;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction1');
				api.store.search.DATA.other.type = 2;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction2');
				api.store.search.DATA.other.type = 3;
				app.HMARTJsObj(api.store.search.DATA,'winLoad.auction3');
				
			},
			auction1 : function(data){ /* 1 － 正在竞拍 */
				api.store.search.param = eval('(' + data +')');
				app.ajax(api.store.search,function(data){
					var html = template('temp-auction-01', data);
					mui('#scroll1')[0].innerHTML = '';
					var div = document.createElement("div");
			　　　　	div.innerHTML = html;
			　　　　	mui('#scroll1')[0].appendChild(div);
					mui("#scroll1")[0].setAttribute('pageIndex', 1);
					mui("#scroll1")[0].setAttribute('data-type', 1);
					winBind.toArtInfo();
				});
				mui('header').on('tap','.scroll_init',function(){
				  	mui('#wrapper1').scroll().scrollTo(0,0,100);//
				}) 
			},
			auction2 : function(data){ /* 2 － 竞买预展 */
				api.store.search.param = eval('(' + data +')');
				app.ajax(api.store.search,function(data){
					var html = template('temp-auction-03', data);
					mui('#scroll2')[0].innerHTML = '';
					var div = document.createElement("div");
			　　　　	div.innerHTML = html;
			　　　　	mui('#scroll2')[0].appendChild(div);
					mui("#scroll2")[0].setAttribute('pageIndex', 1);
					mui("#scroll2")[0].setAttribute('data-type', 2);
//					mui('#wrapper2').scroll().scrollTo(0,0,100);//
					winBind.toArtInfo();
				});
				
				mui('header').on('tap','.scroll_init',function(){
				  	mui('#wrapper2').scroll().scrollTo(0,0,100);//
				}) 
			},
			auction3 : function(data){ /* 3 － 历史竞买 */
				api.store.search.param = eval('(' + data +')');
				app.ajax(api.store.search,function(data){
					var html = template('temp-auction-03', data);
					mui('#scroll3')[0].innerHTML = '';
					var div = document.createElement("div");
			　　　　	div.innerHTML = html;
			　　　　	mui('#scroll3')[0].appendChild(div);
					mui("#scroll3")[0].setAttribute('pageIndex', 1);
					mui("#scroll3")[0].setAttribute('data-type', 3);
					//mui('#wrapper3').scroll().scrollTo(0,0,100);//
					winBind.toArtInfo();
				});
			},
			classfiy : function(data){
				api.store.classfiy.param = eval('(' + data +')');
				app.ajax(api.store.classfiy,function(data){
					
					var limit_top = mui('.limit_top')[0];
					limit_top.innerHTML = '';
					var p0 = document.createElement('p');
					p0.classList.add('mui-control-item');
					p0.innerHTML = '<a class="active">全部</a>';
					limit_top.appendChild(p0);
					
					mui.each(data.obj, function(i,item){
						var p = document.createElement('p');
						p.classList.add('mui-control-item');
						p.innerHTML = '<a code="'+this.id+'" class="">'+this.name+'</a>';
						limit_top.appendChild(p);
					});
					
					mui('.limit_top').on('tap','p',function(){
						var a = this.firstChild;
						var code = a.getAttribute('code');
						if(limit_top.querySelector('.active')) limit_top.querySelector('.active').classList.remove('active');
						a.classList.add('active');
												
						api.store.search.DATA.other.keywork = mui('#search1')[0].value;
						api.store.search.DATA.other.classifyCode = (code==null?'':code);
						winLoad.initData();
					});
				});
			}
		}
		
